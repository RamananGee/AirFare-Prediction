<body style="background-image: linear-gradient(
											45deg,
											#ffbc00,
											#ff0058
										);">
<div>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark" id="pb-navbar">
    <div class="container">
      <a class="navbar-brand ml-0" href="/">Travel Genie</a>
      <button class="navbar-toggler ml-auto" type="button" data-toggle="collapse" data-target="#probootstrap-navbar"
        aria-controls="probootstrap-navbar" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="probootstrap-navbar">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item pr-4"><a class="nav-link" href="https://flight-price-prediction-api.herokuapp.com/">Flight Fare Prediction</a></li>
          
          <li class="nav-item pr-4"><a class="nav-link" href="/faq">FAQ</a></li>
          <li class="nav-item pr-4">
            <div class="dropdown">
              <button class="btn btn-success dropdown-toggle" type="button" id="dropdownMenuButton"
                data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                Profile
              </button>
              <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <a class="dropdown-item" href="/myaccount">My Account</a>
              
                <a class="dropdown-item" href="/flightBookings">Flight Bookings</a>
                <a class="dropdown-item" href="#">Something wrong! Contact Us</a>
                <a href="/logout" class="dropdown-item">Log Out</a>
              </div>
            </div>
            {{!-- {{{loginBtn}}} --}}
          </li>
          {{!-- <li class="nav-item pr-4">
                        {{{signupBtn}}}
                    </li> --}}
        </ul>
      </div>
    </div>
  </nav>
  <div id="back-img">
    <div class="row">
      <div class="col-6">
        <div class="title-text">
          <h1 style="font-family: 'Satisfy'; display: block">Welcome to </h1>
          <p><span
              style="font-size: 90px; padding-left: 40px; margin-top: 240px; display: block; font-family: 'Dancing Script', cursive; color: black; text-shadow: -4px 3px 0 #FF0000 ;">Travel Genie</span></p>
        </div>
      </div>
 <div class="col-6">
        <div class="search-box effect7">
          <h3><kbd>Plan Your Journey</kbd></h3>
          <div class="row">

            <!--    first row  -->
            <form autocomplete="on">
              <div class="row inside form-group ui-widget">
                <div class="col">
                  <input type="text" class="form-control ui-widget" placeholder="From" id="originPlace" class="airport">
                </div>
                <div class="col" id="aright" style="margin-left: 0px; margin-right: 25px;">
                  <input type="text" class="form-control" placeholder="To" id="destinationPlace" class="airport">
                </div>
              </div>
              <!-- second row  -->
              <div class="row date top-fix">
                <div class="col-md-5 inside  form-group">
                  <label for="">Start Date:</label>
                  <input type="date" class="form-control" id="startDate">
                </div>
                <p><span id="position">&#8652;</span></p>
                <div class="col-md-5 inside form-group">
                  <label for="">Return Date:</label>
                  <input type="date" class="form-control" id="returnDate">
                </div>
              </div>
            </form>
            <div class="center" style="margin-left: 170px ; margin-top: -30px">
              <button class="btn btn-primary" style="width: 200px ; font-size: 20px" id="submitBtn">Search</button>
            </div>
          </div>

        </div>
      </div>
      {{{profileBtn}}}
    </div>
  </div>
  <!--filter and search results here -->
<div>
  <center><h3 style="padding-top: 3%; font-family: 'Open Sans', sans-serif;">Book your Flight</h3></center>
 <div class="results m-5 p-5 pb-5" id="FlightResult" >

</div>
                    </div>
 
</div>
